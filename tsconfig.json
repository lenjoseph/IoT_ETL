{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"outDir": "./dist",
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"noImplicitAny": false,
		"types": ["node"],
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"exclude": ["node_modules"]
}